{
  "name": "solo-dungeon",
  "version": "1.0.5",
  "description": "Phaser 3 starter template with TypeScript and webpack.",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --config webpack/webpack.dev.js --port 9998",
    "build": "rimraf dist && webpack --config webpack/webpack.prod.js",
    "bundle": "npm run build",
    "serve-window": "SET PORT=8120 && serve dist",
    "serve": "serve dist -p 8120",
    "android": "npx cap run android",
    "build-android": "npm run build && npm run android"
  },
  "engines": {
    "node": ">=12"
  },
  "license": "MIT",
  "dependencies": {
    "phaser": "^3.55.2"
  },
  "devDependencies": {
    "@capacitor/android": "^3.3.4",
    "@capacitor/cli": "^3.3.4",
    "@capacitor/core": "^3.3.4",
    "@capacitor/status-bar": "^1.0.6",
    "copy-webpack-plugin": "^10.1.0",
    "html-webpack-plugin": "^5.5.0",
    "javascript-obfuscator": "^3.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0",
    "webpack-obfuscator": "^3.5.0",
    "workbox-webpack-plugin": "^6.4.2"
  }
}
